 -- 멤버 테이블 만든다
 CREATE
	TABLE
		"JAVA"."TAB_MEMBER"(
			"MEM_ID" VARCHAR2(20) NOT NULL ENABLE,
			"MEM_PWD" VARCHAR2(20) NOT NULL ENABLE,
			"MEM_NAME" VARCHAR2(50) NOT NULL ENABLE,
			"MEM_PHONE" VARCHAR2(20) NOT NULL ENABLE,
			"LOGIN_DATE" DATE,
			"STATUS_CODE" CHAR(1) DEFAULT '1'
		) SEGMENT CREATION IMMEDIATE PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING STORAGE(
			INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT
		) TABLESPACE "STUDY";

-- 몇개의 데이터 인서트
 INSERT
	INTO
		TAB_MEMBER(
			MEM_ID,
			MEM_PWD,
			MEM_NAME,
			MEM_PHONE,
			LOGIN_DATE,
			STATUS_CODE
		)
	VALUES(
		'admin',
		'1111',
		'admin',
		'111-1111-1111',
		'',
		'2'
	);

-- 테이블 조회
 SELECT
	*
FROM
	TAB_MEMBER;