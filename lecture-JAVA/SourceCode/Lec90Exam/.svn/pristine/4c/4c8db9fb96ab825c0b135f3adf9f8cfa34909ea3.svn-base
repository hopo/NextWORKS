package kr.nextit;

public class SumTest {
	public static void main(String[] args) {
		SumTest st = new SumTest();
		
		int a = 1;
		int b = 10;
				
		int ret = st.sum(a, b);
		System.out.printf("ê²°ê³¼ : %d",ret);
	}
	
	int sum(int a, int b) {
		int ret = 0;
		while(a != b+1) {
			ret += a++;
		}
		
		return ret;
	}
}
