<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<HBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.Ex07DialogRootController">
	
	<padding> <Insets topRightBottomLeft="20" /> </padding>
	<spacing>10</spacing>

	<children>
		<Button text="OpenFileChooser" onAction="#handleOpenFile"/>
		<Button text="SaveFileChooser" onAction="#handleSaveFile"/>
		<Button text="DirectoryChooser" onAction="#handleOpenDirectory"/>
		<Button text="Popup" onAction="#handlePopup"/>
		<Button text="Custom" onAction="#handleCustom"/>
	</children>
</HBox>

