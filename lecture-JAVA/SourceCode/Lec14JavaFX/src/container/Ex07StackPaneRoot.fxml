<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<StackPane prefWidth="500" prefHeight="300"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>

		<!-- preserveRatio: 종횡비를 유지 할 것이냐 (default = true) -->
		<ImageView fitWidth="500" fitHeight="300"
			preserveRatio="true">
			<image>
				<Image url="@../res/images/gnome-tyr_02.png" />
			</image>
		</ImageView>

		<ImageView fitWidth="150" fitHeight="150"
			preserveRatio="true">
			<image>
				<Image url="@../res/images/Nikon.png" />
			</image>
		</ImageView>

	</children>
</StackPane>

