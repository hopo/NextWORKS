<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="345.0" prefWidth="408.0"
	spacing="10.0" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/8.0.141">

	<children>

		<ImageView fitHeight="439.0" fitWidth="313.0"
			pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../res/images/lights.jpg" />
			</image>
		</ImageView>

		<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0"
			spacing="10.0">
			<children>
				<Button mnemonicParsing="false" text="이전" />
				<Button maxWidth="1.7976931348623157E308"
					mnemonicParsing="false" text="다음" HBox.hgrow="ALWAYS" />
			</children>
		</HBox>

	</children>

	<padding>

		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />

	</padding>

</VBox>
